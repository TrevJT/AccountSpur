<%- contentFor('HeaderCss') %>

<%- contentFor('breadcrumb') %>
<div class="row">
    <div class="col-sm-12">
        <div class="page-title-box">
            <div class="btn-group pull-right">
                <ol class="breadcrumb hide-phone p-0 m-0">
                    <li class="breadcrumb-item"><a href="#">Upload</a></li>
                    <li class="breadcrumb-item active">Upload Single Contact</li>
                </ol>
            </div>
            <h4 class="page-title">Enter Customer Information</h4>
       </div>
    </div>
</div>

<%- contentFor('body') %>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card m-b-20">
                            <div class="card-body">

                                <form class="" action="#">
                                    <div class="form-group">
                                        <label>Full Name</label>
                                        <input type="text" class="form-control" required placeholder="Donald Duck"/>
                                    </div>

                                    <div class="form-group">
                                        <label>E-Mail</label>
                                        <div>
                                            <input type="email" class="form-control" required
                                                   parsley-type="email" placeholder="Enter a valid e-mail"/>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label>Mobile Number (9 Digits)</label>
                                        <div>
                                            <input type="text" class="form-control" required
                                                   data-parsley-length="[9,9]"
                                                   placeholder="1234567890"/>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div>
                                            <button type="submit" class="btn btn-pink waves-effect waves-light">
                                                Submit
                                            </button>
                                            <button type="reset" class="btn btn-secondary waves-effect m-l-5">
                                                Cancel
                                            </button>
                                        </div>
                                    </div>
                                </form>

                            </div>
                        </div>
                    </div> <!-- end col -->


                    </div> <!-- end col -->
                </div> <!-- end row -->

<%- contentFor('FooterJs') %>
<script type="text/javascript" src="public/assets/plugins/parsleyjs/parsley.min.js"></script>

<%- contentFor('BottomJs') %>
<script type="text/javascript">
    $(document).ready(function() {
        $('form').parsley();
    });
</script>
